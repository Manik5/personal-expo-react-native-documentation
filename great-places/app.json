{
  "expo": {
    "name": "Great Places Test",
		"slug": "great-places",
		"privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/places.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/icon.png",
				"backgroundColor": "#171717"
			},
			"permissions": [
				"ACCESS_FINE_LOCATION",
				"CAMERA",
				"WRITE_EXTERNAL_STORAGE"
			]
		}
  }
}
